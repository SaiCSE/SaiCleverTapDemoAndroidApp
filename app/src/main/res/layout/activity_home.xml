<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/charged"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <Button
        android:id="@+id/update_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update Profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

    <Button
        android:id="@+id/add_to_cart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="144dp"
        android:text="Add to Cart"
        app:layout_constraintEnd_toEndOf="@+id/update_profile"
        app:layout_constraintStart_toStartOf="@+id/update_profile"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/product_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Product Viewed"
        app:layout_constraintEnd_toEndOf="@+id/add_to_cart"
        app:layout_constraintStart_toStartOf="@+id/add_to_cart"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Charged"
        app:layout_constraintEnd_toEndOf="@+id/product_view"
        app:layout_constraintStart_toStartOf="@+id/product_view"
        tools:layout_editor_absoluteY="322dp" />

    <Button
        android:id="@+id/App_Inbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="App Inbox"
        app:layout_constraintEnd_toEndOf="@+id/button4"
        app:layout_constraintStart_toStartOf="@+id/button4"
        tools:layout_editor_absoluteY="420dp" />

    <Button
        android:id="@+id/Other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other"
        app:layout_constraintEnd_toEndOf="@+id/App_Inbox"
        app:layout_constraintStart_toStartOf="@+id/App_Inbox"
        tools:layout_editor_absoluteY="507dp" />
</androidx.constraintlayout.widget.ConstraintLayout>